<%- include("./components/header")%>
    <div class="container">
        <h1 class="mb-4">Welcome to FaceBook</h1>
        <% if (result) { %>
            <div class="card">
                <div class="card-body">
                  <h5 class="card-title"><%= result.name %></h5>
                  <p class="card-text"><%= result.message %></p>
                  <p><%= result.createdAt.toLocaleDateString() %></p>
                  <a href=/feed/edit/<%= result._id%> class="btn btn-outline-dark">Edit</a>
                  <a href=/feed/delete/<%= result._id%> class="btn btn-outline-dark">Delete</a>
                  <a href="/" class="btn btn-outline-dark">Go home</a>
                </div>
              </div>
            <% } else { %>
                    there is no record
        <% } %>
     </div>
    <%- include("./components/footer")%>